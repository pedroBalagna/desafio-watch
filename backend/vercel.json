{
  "version": 2,
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/tasks/(.*)",
      "destination": "./dist/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "./dist/server.js"
    }
  ]
}
